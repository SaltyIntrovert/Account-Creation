#Used to create a timer for account changes that need to occur after hours

$StartTime = Get-Date -Format "M/dd/yyy HH:MM:ss"
$EndTime = [datetime]"21:30"
$ETAa = New-TimeSpan -Start $StartTime -End $EndTime | Select-Object -ExpandProperty TotalSeconds

IF($ETAa -ge 1){
    $ETA = $ETAa}
elseIF($ETAa -le 0){
    $ETA = 0}

echo $ETA
